<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script type="text/javascript">

	//지정된 반복횟수 만큼 
	for(var i = 0; i < 5 ; i++){
		console.log(i);
	}
	
	//배열을 다루는 유용한 for문
	var names = ["살라","마네","피르미누","스터릿지"];
	for(var index in names){
		console.log(index,names[index]);
	}
</script>
</head>

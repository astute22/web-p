<script type="text/javascript">
	var images = ["a.jpg","b.jpg","c.jpg","d.jpg","e.jpg"];
	
	var gallerybox = document.getElementsById("gallery-box");
	
	var htmlContents="";
	images.forEach(function(imageName, index) {
		htmlContents += "<img src='../images/"+imageName+"'>";
	});
	
	gallerybox.innerHTML = htmlContents;
</script>
